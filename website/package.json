    "gatsby-plugin-sass": "^4.1.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.2.0",
    "sass": "^1.32.8"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/react-helmet": "^6.1.0",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
